<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->

<html lang="vi" class="no-js">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />

    <meta content="Anpero" name="author" />
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css" />
    <script src="/assets/plugins/jquery-1.10.2.min.js"></script>
    @System.Web.Optimization.Styles.Render("~/bundles/global.css")
    @RenderSection("head", false)
    
</head>
<body class="page-container-bg-solid page-header-fixed page-sidebar-closed-hide-logo page-md">
    @Html.Partial("_partial/_patialMenuTop2")<div class="clearfix"></div><div class="page-container">
        @Html.Partial("_partial/_patialSidebarSuperAdmin")
        @RenderSection("featured", required: false)
        <div class="page-content-wrapper">
            <div class="page-content">
                @RenderBody()<div class="clearfix"></div>
            </div>
        </div>
    </div>

    @System.Web.Optimization.Scripts.Render("~/bundles/global.js")
    <script>
        var mains = {};
        var profiles = { curentDeparment: 0, type: 1 }; Anpero.CurentStore = "0"
    </script>
    @RenderSection("scripts", required: false)
    <script src="/Scripts/webScipt.js"></script>
    <script src="/Scripts/main.js"></script>
    <script src="/assets/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js" type="text/javascript"></script>
    <script src="/assets/plugins/bootstrap-datepicker/js/locales/bootstrap-datepicker.vi.js"></script>
    <div class="page-footer">
        <div class="page-footer-inner">
            2016 &copy; Anpero.com Theme By
            <a target="_blank" href="/">Keenthemes</a> &nbsp;|&nbsp;
        </div>
        <div class="scroll-to-top">
            <i class="icon-arrow-up"></i>
        </div>
    </div>

</body>
</html>